@using SFA.DAS.DigitalCertificates.Web.Controllers
@{
    ViewData["Title"] = "Digital Certificates Certificates List";
    ViewBag.GaData.Vpv = "/digital-certificates-certificates-list";
}
<main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-xl">@ViewData["Title"]</h1>
            <a asp-route="@CertificatesController.CertificateRouteGet" class="govuk-button">
                Certificate
            </a>
        </div>
    </div>
</main>